{
  "name": "webend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev --port 8888",
    "prod": "vite --mode prod --port 9888",
    "build": "vite build",
    "deploy": "vite build && vite serve --mode prod",
    "tsc": "tsc --noEmit",
    "lint": "eslint --ext .ts --ext .tsx src",
    "lint:fix": "eslint --ext .ts --ext .tsx src --fix",
    "prettier": "prettier src tests types",
    "preview": "vite preview --mode prod",
    "serve": "vite serve --mode prod",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "typecheck": "vitest typecheck",
    "coverage": "vitest run --coverage",
    "coverage:all": "vitest --coverage.enabled --coverage.all --ui --mode dev",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@ant-design/colors": "^7.0.0",
    "@ant-design/icons": "^5.1.0",
    "@ant-design/plots": "^2.1.14",
    "@ant-design/pro-components": "^2.6.49",
    "@reduxjs/toolkit": "^2.0.1",
    "@uiw/react-md-editor": "3.25.6",
    "ahooks": "^3.6.2",
    "antd": "^5.11.5",
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "lodash-es": "^4.17.21",
    "rc-field-form": "^1.43.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-markdown": "^7.1.2",
    "react-redux": "^9.0.2",
    "react-router": "^6.11.2",
    "react-router-dom": "^6.11.2",
    "redux": "^5.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^3.0.1",
    "remark-math": "5.1.1",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@react-buddy/ide-toolbox": "^2.3.1",
    "@react-buddy/palette-antd": "^5.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.2.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.2.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.26",
    "@types/unist": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "cypress": "^13.6.4",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.3",
    "happy-dom": "^12.10.3",
    "jsdom": "^24.0.0",
    "msw": "^2.0.9",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "terser": "^5.26.0",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.16.0",
    "vite": "^5.1.0-beta.5",
    "vitest": "^1.2.2"
  }
}
